function add(a,b){
   
     return a+b
}
function multi(a,b){

     return a*b
}
function div(a,b){
     return a/b
}


function passNum(a,b,callback){
let c= callback(a,b);
return c;   
}


passNum(5,6,add);
passNum(5,6,multi);
passNum(5,6,div);

//this is simple example to understand.basically it is used in async function
